@model List<VoteResult>

@{
    ViewBag.Title = "Election Results";
}

<h2>Election Results</h2>
<table class="table">
    <thead>
        <tr>
            <th>Candidate</th>
            <th>Total Votes</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model)
        {
            <tr>
                <td>@r.Candidate</td>
                <td>@r.TotalVotes</td>
            </tr>
        }
    </tbody>
</table>
<a asp-action="Index" asp-controller="Voting">Back to Blockchain</a>
